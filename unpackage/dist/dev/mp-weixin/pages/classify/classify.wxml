<view class="container"><view class="left"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><view class="{{[num==index?'left_list activeList':'left_list']}}" data-current="{{index}}" id="{{item.id}}" data-event-opts="{{[['tap',[['getlist',['$event']]]]]}}" bindtap="__e"><label for>{{item.catename}}</label></view></block></view><view class="right"><view class="right_list"><view class="bottom"><block wx:for="{{infolist}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><view class="bottom_list" id="{{item.id}}" data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" bindtap="__e"><view><image src="{{beseUrl+item.img}}" alt></image></view><view class="title"><label class="_span">{{item.catename}}</label></view></view></block></view></view></view></view>